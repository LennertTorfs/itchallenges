@page "/foto"

<h3>Foto</h3>

<div class="row">
    <div class="col">
        <h1>Oefening 2</h1>
    </div>
</div>
<div class="row">
    <div class="col">
        <div class="card">
            <div class="card-header">
                Random foto
            </div>
            <div class="card-body text-center">
                <img src="@imageUrl" id="foto">
                <br>
                <button class="btn btn-primary mt-2" @onclick="ChangeImage">Random foto</button>
                <div id="logging">@((MarkupString)log)</div>
            </div>
        </div>
    </div>
</div>